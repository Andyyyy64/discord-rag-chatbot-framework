{
  "name": "discord-rag-chatbot-framework",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "node --watch --loader ts-node/esm --experimental-specifier-resolution=node ./src/index.ts",
    "build": "tsup",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "check": "eslint . --ext .ts && tsc -p tsconfig.json --noEmit",
    "deploy": "bash scripts/deploy.sh",
    "db:types": "bash scripts/generate-db-types.sh",
    "db:reset": "node --loader ts-node/esm --experimental-specifier-resolution=node ./scripts/reset-db.ts"
  },
  "dependencies": {
    "@google/genai": "^1.29.0",
    "@google/generative-ai": "^0.5.0",
    "@supabase/supabase-js": "^2.46.1",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "js-tiktoken": "^1.0.10",
    "p-limit": "^4.0.0",
    "tslog": "^4.9.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "supabase": "^2.54.11",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsup": "^8.1.0",
    "typescript": "^5.6.3"
  }
}
